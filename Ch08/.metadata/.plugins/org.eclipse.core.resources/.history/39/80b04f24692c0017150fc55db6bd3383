		return 0;
	}
	
	private static int iterate(int[] numbers, int index) {
		if (index < numbers.length) {
			if (numbers[index] > maxNumberInRange) maxNumberInRange = numbers[index];
			index++;
			iterate(numbers, index);
		}
		return max;
	}
}
