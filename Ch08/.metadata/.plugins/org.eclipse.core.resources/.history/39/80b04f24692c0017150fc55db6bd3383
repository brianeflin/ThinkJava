
public class Main {
	
	static int maxNumberInRange;

	public static void main(String[] args) {
		
		int[] numbers = {5, 6, 4, 7, 3, 8, 2, 9, 1, 10};
		maxNumberInRange = numbers[0];
		System.out.println(iterate(numbers, 0));
	}

