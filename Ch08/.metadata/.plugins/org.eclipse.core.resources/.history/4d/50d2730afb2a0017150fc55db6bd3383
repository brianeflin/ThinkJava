
public class Main {

	public static void main(String[] args) {
		
		boolean[] prime = sieve(30);
		
		for (int i = 0; i < prime.length; i++) {
			System.out.println(i + ": " + prime[i]);
		}
	}

	private static boolean[] sieve(int n) {
		
		boolean[] a = new boolean[n-2];
		
		for (int i = 2; i <= n; i++) {
			a[i] = true;
		}
		
		
		
		return a;
	}
}
