
public class Main {

	public static void main(String[] args) {
		
		double[] a = {1, 2, 3, 4, 5};
		powerArray(a);
		
		for (double num : a) {
			System.out.print(num + ", ");
		}
	}

	public static double[] powerArray(double[] a) {
		for (int i = 0; i < a.length; i++) {
			a[i] = Math.pow(a[i], 2);
		}
		return a;
	}
}
