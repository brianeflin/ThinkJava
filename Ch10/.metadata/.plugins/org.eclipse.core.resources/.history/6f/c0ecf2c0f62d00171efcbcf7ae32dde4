
public class Main {

	public static void main(String[] args) {
			
		for (int i = 1; i < 10; i++) {
			System.out.println(i + "! = " + factorial(i));
		}
	}
	
	private static int factorial(int num) {
		
		int result = num;
		
		for (int i = num - 1; i > 1; i--) {
			result *= i;
		}
		
		return result;
	}
}
