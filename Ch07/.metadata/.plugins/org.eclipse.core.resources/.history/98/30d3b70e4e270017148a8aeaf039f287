
public class Main {

	public static void main(String[] args) {
		
		System.out.println(factorial(5));
	}
	
	private static double myExp(int exp, int iterations) {
		
		double answer = 1 + exp;
		
		for (int i = 2; i <= iterations; i++) {
			
		}
		
		return answer;
	}

	private static int factorial(int num) {
		
		int answer = 1;
		
		for (int i = num; i > 0; i--) {
			answer *= i;
		}
		
		return answer;
	}
}
